<!-- Custom CSS -->
  <link rel="stylesheet" type="text/css" href="../static/css/styles.css">
{{#each place}}
<div class="row">
  <div class="col md 2">
    <img class="thumbs" src="{{ pictures }}">
  </div>
  <div class="col md 10">
    <h3>{{ name }}</h3>
    <p>{{ address }}</p>
  </div>
</div>
<div class="row">
  <div class="col md 12 offset-m2">
    <h5>Reviews</h5>
  </div>
</div>
<div class="row">
  <div class="col md 2 offset-m2">
    {{#each users}}
    <p>{{ userName }}</p>
    {{/each}}
    {{/each}}
    </div>
    <div class="col md 8">
      {{#each ratings}}
      <p><span class="stars">{{ stars }}</span> <span class="comment">{{ comment }}</span></p>
      {{/each}}
    </div>
  </div>
</div>
<script type="text/javascript" src="https://code.jquery.com/jquery-2.2.1.min.js"></script>
<script type="text/javascript" src="../static/js/index.js"></script>