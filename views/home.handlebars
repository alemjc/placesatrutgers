<!-- This is the main handlebars page -->
<div class="row">
  <div class="col md 12">
    <h1>Places</h1>
  </div>
</div>
    {{#each place}}
    <div class="row">
      <div class="col md 2">
        <img class="thumbs" src="{{ pictures }}">
      </div>
        <div class="col md 10">
          <h5>{{ name }}</h5>
          <ul>
            <li>{{ address }}</li>
            <li>{{ category }}</li>
          </ul>
        </div>
      </div>
    </div>
    {{/each}}
  </div>